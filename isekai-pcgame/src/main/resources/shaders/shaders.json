{
    "color": {
        "vert": "color",
        "frag": "color",
        "attributes": ["vertex", "color_"],
        "uniforms": ["projection", "modelView"]
    },
    "inst_color": {
        "parent": "color",
        "vert": "inst_color",
        "uniforms_to_attribs": ["modelView"]
    },

    "texture": {
        "vert": "texture",
        "frag": "texture",
        "attributes": ["vertex", "texture_"],
        "uniforms": ["projection", "modelView"]
    },
    "inst_texture": {
        "parent": "texture",
        "vert": "inst_texture",
        "uniforms_to_attribs": ["modelView"]
    },

    "texture_color": {
        "vert": "texture_color",
        "frag": "texture_color",
        "attributes": ["vertex", "color_", "texture_"],
        "uniforms": ["projection", "modelView"]
    },
    "inst_texture_color": {
        "parent": "color",
        "vert": "inst_texture_color",
        "uniforms_to_attribs": ["modelView"]
    },


    "font": {
        "vert": "font",
        "frag": "texture_color",
        "attributes": ["vertex", "glyph_info", "color_", "model"],
        "uniforms": ["projection", "texture_size", "font_height"]
    }
}